<link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,600,700,900" rel="stylesheet">
<link rel="stylesheet" href="https://cdn.datatables.net/1.10.15/css/dataTables.bootstrap.min.css">
<link rel="stylesheet" href="https://cdn.datatables.net/responsive/2.1.1/css/responsive.bootstrap.min.css">
<script src="https://use.fontawesome.com/57ca66c410.js"></script>



<input type="hidden" value="<%=municipio%>" id="municipio"><!--Agregado por GMV-->
<div class="container-table-result">
    <div class="container">
        <legend><h3>Listado de √Ålbums</h3></legend>
        <a href="/Foto/Agregar/<%=municipio%>"><h5>Nuevo</h5></a>
        <div class="row">
            <div class="col-md-12">
                <div class="table-responsive">
                    <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Titulo</th>
                                <th>Habilitado</th>
                                <th>Feccreado</th>
                                <th>Fecmodificado</th>
                                <th>Ver</th>
                            </tr>
                        </thead>
                        <tbody id="tbTramitesPortal">
                            <% if(albums.length>0){%>                               
                               <% albums.forEach(function(album){ %>
                                    <tr>
                                        <td><%=album.titulo%></td>
                                        <td><%=album.habilitado%></td>
                                        <td><%=album.feccreado%></td>
                                        <td><%=album.fecmodificado%></td>
                                       <td><a href="/Album/Editar/<%=album.id%>"><i class="fa fa-file-text-o" aria-hidden="true"></i></a></td>
                                    </tr>
                               <% }) %>                               
                            <% } %>                            
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
<script src="https://cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.10.15/js/dataTables.bootstrap.min.js"></script>
<script src="https://cdn.datatables.net/responsive/2.1.1/js/dataTables.responsive.min.js"></script>
<script src="https://cdn.datatables.net/responsive/2.1.1/js/responsive.bootstrap.min.js"></script>
